                              <Label layoutX="181.0" layoutY="-1.0" text="Filter woord" />
                              <JFXCheckBox fx:id="cbAcceptedFilter" layoutX="126.0" layoutY="-1.0" />
                              <Label layoutX="14.0" layoutY="-1.0" text="Gevalideerd" />
                           </children>
                        </AnchorPane>
                           </children></AnchorPane>
                   </content>
               </Tab>
           </tabs>
       </JFXTabPane>


   </children>

   <stylesheets>
      <URL value="@style.css" />
   </stylesheets>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
