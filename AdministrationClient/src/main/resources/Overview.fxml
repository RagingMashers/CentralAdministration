                           </children></AnchorPane>
                   </content>
               </Tab>
               <Tab text="Bronnen">
                   <content>
                       <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                               <ScrollPane fx:id="scrollPane" layoutX="14.0" layoutY="78.0" prefHeight="590.0" prefWidth="1070.0" vbarPolicy="ALWAYS">
                                   <content>
                                       <AnchorPane fx:id="totalContent" prefHeight="-1.0" prefWidth="-1.0">
                                           <children>
                                               <VBox fx:id="contentHolderR1" prefHeight="-1.0" prefWidth="350.0" />
                                               <VBox fx:id="contentHolderR2" layoutX="359.0" prefHeight="-1.0" prefWidth="350.0" />
                                               <VBox fx:id="contentHolderR3" layoutX="718.0" prefHeight="-1.0" prefWidth="350.0" />
                                           </children>
                                       </AnchorPane>
                                   </content>
                               </ScrollPane>
                        <AnchorPane layoutX="15.0" layoutY="8.0" prefHeight="66.0" prefWidth="1070.0">
                           <children>
                              <DatePicker fx:id="endDate" layoutX="579.0" layoutY="34.0" minHeight="-Infinity" prefHeight="30.0" prefWidth="200.0"/>
